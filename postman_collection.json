		{
			"name": "1. Criar Usuário",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"",
							"const base = `tester_${Date.now()}`;",
							"pm.collectionVariables.set(\"userBase\", base);",
							"pm.collectionVariables.set(\"username\", base);",
							"pm.collectionVariables.set(\"email\", `${base}@example.com`);",
							"pm.collectionVariables.set(\"altUsername\", `alt_${base}`);",
							""
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"",
							"pm.test(\"Status 201 Created\", function () {",
							"  pm.response.to.have.status(201);",
							"});",
							"const json = pm.response.json();",
							"pm.collectionVariables.set(\"userId\", json.id);",
							"pm.test(\"Tem ID no corpo\", function () {",
							"  pm.expect(json.id).to.be.ok;",
							"});",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n  \"username\": \"{{username}}\",\n  \"name\": \"{{name}}\",\n  \"email\": \"{{email}}\",\n  \"userType\": \"{{userType}}\",\n  \"password\": \"{{password}}\",\n  \"address\": {\n    \"street\": \"{{street}}\",\n    \"neighborhood\": \"{{neighborhood}}\",\n    \"city\": \"{{city}}\",\n    \"number\": \"{{number}}\",\n    \"zipCode\": \"{{zipCode}}\"\n  }\n}"
